<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
  <connectionStrings>
    <add name="PackageTracker" providerName="System.Data.MySql" connectionString="SERVER=127.0.0.1;DATABASE=PackageTracker;UID=root;Password=Password"/>
  </connectionStrings>
  <appSettings>
    <!-- Default Reader Name. Used if multiple readers exist in the system. -->
    <add key="RFIDReaderName" value="OMNIKEY CardMan 5x25-CL 0"/>
    <!-- Email Settings-->
    <add key="EmailFrom" value="Mailroom@svc.edu"/>
    <add key="EmailSubject" value="SVC Mailroom Parcel Notification"/>
    <add key="SmtpServer" value="127.0.0.1"/>
  </appSettings>
</configuration>